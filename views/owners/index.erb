<h1 id="subtitle">Owners</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Pets</th>
  </tr>
  <% for owner in @owners %>
  <tr>
    <td><a href="/owners/<%= owner.id %>"><%= owner.name %></a></td>
    <td><%for animal in owner.animals %>
        <a href="/animals/<%= animal.id %>">  <%= animal.name %></a>
        <% end %></a>
      </td>
  </tr>
  <% end %>
</table>
